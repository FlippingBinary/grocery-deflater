{
  "expo": {
    "name": "grover-app",
    "slug": "grover-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-new.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-new.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/splash-new.png",
        "resizeMode": "cover"
      },
      "bundleIdentifier": "org.grover.exp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-new.png",
        "backgroundColor": "#FFFFFF"
      },
      "splash": {
        "mdpi": "./assets/android/drawable-mdpi/splash-new-mdpi.png",
        "hdpi": "./assets/android/drawable-hdpi/splash-new-hdpi.png",
        "xhdpi": "./assets/android/drawable-xhdpi/splash-new-xhdpi.png",
        "xxhdpi": "./assets/android/drawable-xxhdpi/splash-new-xxhdpi.png",
        "xxxhdpi": "./assets/android/drawable-xxxhdpi/splash-new-xxxhdpi.png",
        "resizeMode": "cover"
      },
      "package": "org.grover.exp"
    },
    "web": {
      "favicon": "./assets/favicon-new.png"
    }
  }
}
